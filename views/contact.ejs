
<%- include("partials/header.ejs") %>

<div class="container-fluid">
  <div class="row backgroundOfContactForm">

      <div class="col-sm justify-content-center">
        <div class="card-deck mb-3" id="leftOfContactForm">
          <h2>Contact us</h2>
        </div>
      </div>
      <div class="col-sm" id="contactForm">
       

        <form action="/send" method="post">
          <div class="form-group">
            <label for="name">Name</label>
            <input type="text" class="form-control" id="name" name="name" value="">
          </div>
          <div class="form-group">
            <label for="exampleInputEmail1">Email address</label>
            <input type="email" class="form-control" id="exampleInputEmail1" name="email" aria-describedby="emailHelp">
          </div>
          <div class="form-group">
            <label for="messageSubject">Title</label>
            <input type="text" class="form-control" id="messageSubject" name="messageSubject">
          </div>
          <div class="form-group">
            <label for="message">Message</label>
            <textarea class="form-control" name="message" id="message"></textarea>
          </div>
         <button type="submit" class="btn btn-primary">Submit</button>
        </form>

      
    </div>
  </div>


  <div class="row justify-content-center" id="contactAddress">
    <div class="col-10 col-md-8 col-lg-4">
      <div class="card-deck mb-3 text-center">
        <div class="card mb-4 shadow-sm">
          <div class="card-header" id="contactusCard">
            <h2 class="my-0 font-weight-normal">Address</h2>
          </div>
          <div class="card-body contactUsContent">
            <ul class="list-unstyled mt-3 mb-4">
              <p>C 19-9/Door No. 3002 <br>
                Kishangarh, Vasant Kunj <br>
                New Delhi <br>
                110070 <br>
                <br><br>
                MAILING ADDRESS
                <br>
                contact.finerblue@gmail.com</p>

            </ul>
          </div>
        </div>

      </div>
    </div>

  </div>

</div>

